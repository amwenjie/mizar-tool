{
    "name": "alcor-template-app",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": ""
    },
    "author": "jaywen",
    "description": "a typescript template for mizar app",
    "license": "MIT",
    "scripts": {
        "build": "alcor build",
        "debug": "alcor build -dr"
    },
    "dependencies": {
        "core-js": "^3.8.2",
        "mizar": "~0.0.8",
        "react": "~17.0.1",
        "react-dom": "~17.0.1",
        "react-redux": "~7.2.2",
        "react-router": "~5.2.0",
        "react-router-config": "~5.1.1",
        "react-router-dom": "~5.2.0",
        "redux": "~4.0.5",
        "redux-thunk": "~2.3.0"
    },
    "devDependencies": {
        "@types/mocha": "~2.2.34",
        "@types/react": "~17.0.0",
        "@types/react-dom": "~17.0.0",
        "@types/react-redux": "~7.1.15",
        "@types/react-router": "~5.1.11",
        "@types/react-router-config": "~5.0.2",
        "@types/react-router-dom": "~5.1.7",
        "alcor": "~0.1.0"
    },
    "browserslist": [
      "Android 2.3",
      "Android >= 4",
      "Chrome >= 20",
      "Firefox >= 24",
      "Explorer >= 8",
      "iOS >= 6",
      "Opera >= 12",
      "Safari >= 6"
    ],
    "customConfig": {
      "debugPort": 9026,
      "vendorPack": {
        "minify": true,
        "browserVendor": []
      },
      "tslint": {
        "disable": false
      },
      "clientPack": {
        "minify": true,
        "cssModule": true,
        "chunk": true,
        "style": {
          "sourceMap": true,
          "base64": false,
          "ieCompat": true
        }
      },
      "serverPack": {
        "minify": true,
        "templateMinify": true
      }
    }
}
